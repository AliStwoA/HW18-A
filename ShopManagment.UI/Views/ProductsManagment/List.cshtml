@model List<Product>


<h1>مدیریت محصولات</h1>

<table class= table table-bordered table-responsive table-primary table-hover> 
    <thead>
        <tr>
            <th>شناسه</th>
             <th>عنوان کلا</th>
              <th>قیمت</th>
               <th>تعداد موجودی</th>
        </tr>

    </thead>
    <tbody>
        @foreach(var item in @Model)
        {
                  <tr>
<td>@item.Id</td>
<td>@item.Title</td>
<td>@item.Price</td>
<td>@item.Qty</td>

        </tr>
        }


    </tbody>

</table>
